{
  "name": "surveys-results",
  "version": "1.0.0",
  "description": "Gui for showing survey results",
  "main": "index.js",
  "scripts": {
    "test": "standard && API_BASE_URL='http://example.com' jest",
    "test:watch": "standard && API_BASE_URL='http://example.com' jest --watch",
    "build": "webpack --config webpack.prod.js",
    "dev": "API_BASE_URL='http://localhost:8080/api' webpack-dev-server --open --config webpack.dev.js",
    "start": "npm run build && http-server ./dist",
    "lint:fix": "standard --fix"
  },
  "engines": {
    "node": "~9.4.0",
    "npm": "~5.6.0"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "http-server": "^0.11.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "clean-webpack-plugin": "^0.1.17",
    "copy-webpack-plugin": "^4.3.1",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^22.1.4",
    "nock": "^9.1.6",
    "regenerator-runtime": "^0.11.1",
    "standard": "^10.0.3",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1",
    "webpack-merge": "^4.1.1"
  },
  "standard": {
    "globals": [
      "jest",
      "describe",
      "it",
      "expect",
      "before",
      "after",
      "beforeEach",
      "beforeAll",
      "afterEach",
      "afterAll"
    ]
  }
}
